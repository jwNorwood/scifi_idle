[editor_plugins]

enabled=PackedStringArray("res://addons/Todo_Manager/plugin.cfg", "res://addons/scene_manager/plugin.cfg")

[physics]

2d/default_gravity=0.0

[rendering]

renderer/rendering_method="mobile"
